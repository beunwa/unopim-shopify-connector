{
    "name": "unopim/prestashop-connector",
    "description": "A PrestaShop integration package for Unopim, enabling seamless product data synchronization between Unopim and PrestaShop.",
    "type": "library",
    "license": "MIT",    
    "authors": [
        {
            "name": "UnoPim",
            "email": "support@webkul.com"
        }
    ],
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/unopim/unopim-shopify-connector"
        }
    ],
    "autoload": {
        "psr-4": {
            "Webkul\\Prestashop\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Webkul\\Prestashop\\Providers\\PrestashopServiceProvider"
            ],
            "aliases": {}
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "@php artisan vendor:publish --tag=prestashop-config --force",
            "@php artisan prestashop-package:install"
        ]
    },
    "minimum-stability": "dev"    
}
